{
	"compilerOptions": {
		"declaration": false,
		"forceConsistentCasingInFileNames": true,
		"lib": ["ES2020"],
		"module": "CommonJS",
		"moduleResolution": "Node",
		"outDir": "dist/",
		"resolveJsonModule": true,
		"rootDir": "./",
		"target": "ES2020",
		"downlevelIteration": true,
		"esModuleInterop": true
	}
}
